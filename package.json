{
  "name": "symplecv",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "description": "Building and managing resumes the simplest way.",
  "author": "SympleCV",
  "build": {
    "extraMetadata": {
      "main": "build/main.js"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "SympleCV-beta",
      "owner": "SympleCV"
    }
  },
  "dependencies": {
    "@atlaskit/css-reset": "^2.0.0",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.7",
    "@electron/remote": "^2.0.1",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@fontsource/inter": "^4.5.0",
    "@react-pdf/renderer": "^2.0.19",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.6",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-transition-group": "^4.4.3",
    "axios": "^0.26.1",
    "custom-electron-titlebar": "^3.2.7",
    "docx": "^6.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-spellchecker": "^2.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "es-abstract": "^1.19.5",
    "fast-xml-parser": "^4.0.7",
    "file-saver": "^2.0.5",
    "focus-visible": "^5.2.0",
    "formik": "^2.2.9",
    "framer-motion": "4",
    "immutability-helper": "^3.1.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-month-picker": "^2.2.1",
    "react-pdf": "^5.3.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.2",
    "react-xml-parser": "^1.1.8",
    "react-zoom-pan-pinch": "^2.1.3",
    "simplebar": "^5.3.5",
    "simplebar-react": "^2.3.5",
    "styled-components": "^5.3.1",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1",
    "webfontloader": "^1.6.28",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "electron .",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "package": "electron-builder build  --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "make": "electron-forge make",
    "start-electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "mkdir build/src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "lint": "eslint . --ext .js,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/electron-spellchecker": "^1.1.2",
    "@types/file-saver": "^2.0.3",
    "@types/react": "^17.0.19",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-router-dom": "^5.1.9",
    "@types/styled-components": "^5.1.14",
    "@types/uuid": "^8.3.1",
    "@types/webfontloader": "^1.6.33",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "electron": "^14.0.0",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "tsconfig.json": "^1.0.10"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "openresumeapp"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
